#include<WiFi.h>
#include <HTTPClient.h>
#include <ArduinoJson.h>

const char* ssid = "Balamantra";         //"Wokwi-GUEST";
const char* pass = "123456789x";         //"";

unsigned const long interval = 50000;
unsigned long zero = 0;

void setup(){
  Serial.begin(115200);
  WiFi.begin(ssid, pass);
  while(WiFi.status() != WL_CONNECTED){
    delay(100);
    Serial.println(".");
  }
  Serial.println("WiFi Connected!");
  Serial.println(WiFi.localIP());
  
}

void loop(){

  if(millis()-zero > interval){

    HTTPClient http;
    
    //https://newsapi.org/v2/top-headlines/sources?category=businessapiKey=API_KEY        ----->>>> category
    //https://newsapi.org/v2/top-headlines?country=id&apiKey=2a1f6498a3444d26b8b14ee32948525b   ------>>>>> country untuk mnggabungkan dengan type resourc lainnya gunakan pemisah "&"
    
    http.begin("https://newsapi.org/v2/top-headlines?country=id&category=general&articles=articles_4&apiKey=2a1f6498a3444d26b8b14ee32948525b");//"http://jsonplaceholder.typicode.com/todos/1");   
    int httpResponCode = http.GET();
    Serial.println(httpResponCode);
    if(httpResponCode > 0){
 
      const size_t capacity = JSON_ARRAY_SIZE(20) + 20*JSON_OBJECT_SIZE(2) + JSON_OBJECT_SIZE(3) + 20*JSON_OBJECT_SIZE(8) + 16350;
      DynamicJsonBuffer jsonBuffer(capacity);
      
      String json= http.getString();   
      JsonObject& root = jsonBuffer.parseObject(json);
      
      const char* status = root["status"]; // "ok"
      int totalResults = root["totalResults"]; // 37

      JsonArray& articles = root["articles"];
      
      JsonObject& articles_0 = articles[0];
      const char* articles_0_title = articles_0["title"];
    
      JsonObject& articles_1 = articles[1];
      const char* articles_1_title = articles_1["title"];
 
      Serial.println(status);
      Serial.println(totalResults);
      Serial.println( articles_0_title);
      Serial.println( articles_1_title);
     
    }else{
      Serial.print("error ");
      Serial.println(httpResponCode);
    }
  
    zero = millis();
  }

    
}



